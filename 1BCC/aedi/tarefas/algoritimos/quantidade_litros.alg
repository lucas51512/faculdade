INICIO 
	VAR
		VELOCIDADE_MEDIA, TEMPO, DISTANCIA, VELOCIDADE: inteiro;
	CONST
		 LITROS_USADOS: inteiro;
	
	IMPRIMA "Informe o Tempo", TEMPO;
	IMPRIMA "Informe a Velocidade", VELOCIDADE; 
	
	LEIA TEMPO;
	LEIA VELOCIDADE;
	
	DISTANCIA <- TEMPO*VELOCIDADE;
	
	VELOCIDADE_MEDIA <- DISTANCIA/TEMPO
	
	LITROS_USADOS <- 12;
	
	IMPRIMA TEMPO, DISTANCIA, VELOCIDADE_MEDIA, LITROS_USADOS;
	
FIM.